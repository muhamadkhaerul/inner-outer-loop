#include <iostream>

using namespace std;

int main()
{
    //break//
    cout << "Break" << endl;
    for (int a = 1; a <= 9; a++) {if (a == 5) {break;} cout << a << endl;}
    //continue//
    cout << "Continue" << endl;
    for (int a = 1; a <= 9; a++) {if (a == 5) {continue;} cout << a << endl;}
    cin.get();
    return 0;
}
